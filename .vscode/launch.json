{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "mili reader",
      "type": "python",
      "request": "launch",
      "program": "src/mili/reader.py",
      "console": "integratedTerminal"
    },
    {
      "name": "reader test",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/src",
      "program": "tests/reader_test.py",
      "args": [
        "ParallelSingleStateFile.test_nodes_label"
      ],
      "console": "integratedTerminal"
    }
  ]
}